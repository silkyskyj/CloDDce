<GroupBox x:Class="IL2DCE.Pages.Controls.GeneralSettingsGroupBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:IL2DCE.Pages.Controls"
             xmlns:Controls="clr-namespace:IL2DCE.Pages.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="240" d:DesignWidth="1856" Loaded="GroupBox_Loaded"  Header="General Settings" Foreground="#FFA0854A" BorderBrush="#FFA0854A" Height="240">
    <Grid Margin="0,0,0,0">
        <Label Content="[ Additional Random Units ]" HorizontalAlignment="Left" x:Name="labelSelectAddKeep" VerticalAlignment="Top" FontSize="18" Foreground="#B2FFFFFF" Margin="10,84,0,0" />
        <Label Content="[ Max Operations / Units ]" HorizontalAlignment="Left" x:Name="labelSelectAdditionalOperation" VerticalAlignment="Top" FontSize="18" Foreground="#B2FFFFFF" Margin="10,4,0,0" />
        <Controls:HelpButton x:Name="helpButtonAddOperations" Content="?" HorizontalAlignment="Left" Margin="230,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.219,-0.275" Message="An Air Operation may contain 1 to 3 Air Groups, both friendly and enemy, depending on the mission type.\nEach Air Group must be defined in the Mission file.\nGround is the combined number of Ground Group and Stationay Unit, and will be added only when possible." TabIndex="10"/>
        <Label Content="Air" HorizontalAlignment="Left" Margin="14,42,0,0" x:Name="labelSelectAdditionalOperationAirGroup" VerticalAlignment="Top" FontSize="20" Foreground="#B2FFFFFF" />
        <ComboBox HorizontalAlignment="Left" Margin="54,45,0,0" x:Name="comboBoxSelectAdditionalAirOperations" VerticalAlignment="Top" Width="50" FontSize="20" Foreground="#FFA0854A" Background="#00000000" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" TabIndex="12" MaxDropDownHeight="800"/>
        <Label Content="Ground" HorizontalAlignment="Left" Margin="120,42,0,0" x:Name="labelSelectAdditionalOperationGroundGroup" VerticalAlignment="Top" FontSize="20" Foreground="#B2FFFFFF" />
        <Controls:ComboBoxEx HorizontalAlignment="Left" Margin="200,44,0,0" x:Name="comboBoxSelectAdditionalGroundOperations" VerticalAlignment="Top" Width="60" FontSize="20" Foreground="#FFA0854A" Background="#00000000" IsEditable="True" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" TabIndex="14" IsTabStop="True" MaxDropDownHeight="800" />
        <CheckBox x:Name="checkBoxAdditionalAirGroups" Content="Air Group" HorizontalAlignment="Left" Margin="10,122,0,0" VerticalAlignment="Top" FontSize="18" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="16"/>
        <Controls:HelpButton x:Name="helpButtonAdditionalUnits" Content="?" HorizontalAlignment="Left" Margin="244,90,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.219,-0.275" Message="If this is enabled, random unit type will be automatically added when there are few groups/units in the mission file.\nThe aircraft in the air groups added by default are aircraft that the player cannot pilot in this game.\nIf you want to change the unit type added, set it in conf.ini or each campaign files." TabIndex="15"/>
        <CheckBox x:Name="checkBoxAdditionalGroundGroups" Content="Ground Grp" HorizontalAlignment="Left" Margin="120,122,0,0" VerticalAlignment="Top" FontSize="18" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="17"/>
        <CheckBox x:Name="checkBoxAdditionalStationaryUnits" Content="Stationary" HorizontalAlignment="Left" Margin="248,122,0,0" VerticalAlignment="Top" FontSize="18" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="18"/>

        <Label Content="[ Randomize Air Group spawn ]" HorizontalAlignment="Left" Margin="666,4,0,0" x:Name="labelSelectSpawnRandom" VerticalAlignment="Top" FontSize="20" Foreground="#B2FFFFFF" />
        <Controls:HelpButton x:Name="helpButtonSpawnRandom" Content="?" HorizontalAlignment="Left" Margin="956,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.219,-0.275" Message="Set the spawn to be random.\nThe location will move to an available airport within the range of all Air Groups (no other Air Groups within 1500mm), but if there is no vacancy it will be set to AirStart or SetOnPark.\n*Currently, terrain checks are not performed when placing, so it may be placed on uneven ground such as puddles.\nThe altitude will be set within the altitude range for each aircraft mission in the AircraftInfo.ini file, or at a low altitude close to the ground.\nGround Units will be added as Generic type." TabIndex="40"/>
        <Button x:Name="buttonRandomizeAllCheck" Content="+" HorizontalAlignment="Left" Margin="983,10,0,0" VerticalAlignment="Top" Width="25" Height="25" Foreground="#B2FFFFFF" Background="Transparent" TabIndex="42" Click="buttonRandomizeAllCheck_Click"/>
        <Button x:Name="buttonRandomizeAllUnCheck" Content="-" HorizontalAlignment="Left" Margin="1010,10,0,0" VerticalAlignment="Top" Width="25" Height="25" Foreground="#B2FFFFFF" Background="Transparent" TabIndex="44" Click="buttonRandomizeAllUnCheck_Click"/>
        <Label Content="Location:" HorizontalAlignment="Left" Margin="658,46,0,0" x:Name="labelSelectSpawnRandomLocation" VerticalAlignment="Top" FontSize="20" Foreground="#B2FFFFFF" HorizontalContentAlignment="Right" Width="118" />
        <CheckBox x:Name="checkBoxSpawnRandomLocationFriendly" Content="Friendly" HorizontalAlignment="Left" Margin="776,50,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="45"/>
        <CheckBox x:Name="checkBoxSpawnRandomLocationEnemy" Content="Enemy" HorizontalAlignment="Left" Margin="873,50,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="46" IsChecked="True"/>
        <CheckBox x:Name="checkBoxSpawnRandomLocationPlayer" Content="Player" HorizontalAlignment="Left" Margin="960,50,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="47"/>
        <Label Content="Altitude:" HorizontalAlignment="Left" Margin="658,86,0,0" x:Name="labelSelectSpawnRandomAltitude" VerticalAlignment="Top" FontSize="20" Foreground="#B2FFFFFF" RenderTransformOrigin="0.5,0.5" HorizontalContentAlignment="Right" Width="118" >
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.47"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-0.492"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <CheckBox x:Name="checkBoxSpawnRandomAltitudeFriendly" Content="Friendly" HorizontalAlignment="Left" Margin="776,90,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="48"/>
        <CheckBox x:Name="checkBoxSpawnRandomAltitudeEnemy" Content="Enemy" HorizontalAlignment="Left" Margin="873,90,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="49" IsChecked="True"/>
        <Label Content="Time(delay):" HorizontalAlignment="Left" Margin="658,126,0,0" x:Name="labelSelectSpawnRandomTime" VerticalAlignment="Top" FontSize="20" Foreground="#B2FFFFFF" RenderTransformOrigin="0.5,0.5" HorizontalContentAlignment="Right" Width="118" >
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.47"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-0.492"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <CheckBox x:Name="checkBoxSpawnRandomTimeFriendly" Content="Friendly" HorizontalAlignment="Left" Margin="776,130,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="50"/>
        <CheckBox x:Name="checkBoxSpawnRandomTimeEnemy" Content="Enemy" HorizontalAlignment="Left" Margin="873,130,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="51" IsChecked="True"/>
        <Controls:ComboBoxEx Height="30" HorizontalAlignment="Left" Margin="776,166,0,0" x:Name="comboBoxSelectRandomTimeBegin" VerticalAlignment="Top" Width="72" FontSize="20" Foreground="#FFA0854A" Background="#00000000" IsEditable="True" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" TabIndex="52" IsTabStop="True" MaxDropDownHeight="800" />
        <Label Content=" - " HorizontalAlignment="Left" Margin="836,147,0,0" x:Name="labelSelectPeriodRange" VerticalAlignment="Top" Width="50" FontSize="36" Foreground="#B2FFFFFF" RenderTransformOrigin="2.623,0.795" Height="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
        <Controls:ComboBoxEx Height="30" HorizontalAlignment="Left" Margin="874,166,0,0" x:Name="comboBoxSelectRandomTimeEnd" VerticalAlignment="Top" Width="72" FontSize="20" Foreground="#FFA0854A" Background="#00000000" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" TabIndex="53" IsTabStop="True" IsSynchronizedWithCurrentItem="True" IsEditable="True" MaxDropDownHeight="800" />
        <Label Content="[sec]" HorizontalAlignment="Left" Margin="942,165,0,0" x:Name="labelSelectRangeUnit" VerticalAlignment="Top" FontSize="20" Foreground="#B2FFFFFF" RenderTransformOrigin="2.623,0.795" />
        <Controls:HelpButton x:Name="helpButtonSpawnRandomTime" Content="?" HorizontalAlignment="Left" Margin="990,170,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.219,-0.275" Message="Setting this Delay may result in the timing and opportunity of a match differing significantly from what is assumed in the Mission file." TabIndex="55"/>
        <Button x:Name="buttonAllDefault" Content="#" HorizontalAlignment="Left" Margin="1815,10,0,0" VerticalAlignment="Top" Width="25" Height="25" Foreground="#B2FFFFFF" Background="Transparent" TabIndex="104" Click="buttonAllDefault_Click"/>
        <CheckBox x:Name="checkBoxAutoReArm" Content="Auto re-Arm" Margin="1644,46,69,0" VerticalAlignment="Top" FontSize="20" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="71" IsChecked="False"/>
        <CheckBox x:Name="checkBoxAutoReFuel" Content="Auto re-Fuel" HorizontalAlignment="Left" Margin="1644,86,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="73" IsChecked="False"/>
        <Controls:HelpButton x:Name="helpButtonAutoReArmRefuel" Content="?" HorizontalAlignment="Left" Margin="1815,46,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.219,-0.275" Message="If this is enabled, after landing at a friendly airport, if you turn off the magneto and set the throttle value to 0, re-fuel and re-arm (currently only bullets) will occur automatically after a waiting period.\nThe default waiting period is about 300 seconds.(set it in conf.ini)\nRe-fuel will only occur if the aircraft's fuel level is below the minimum.\nThe amount of fuel after refueling will be a value between the minimum fuel level and a full tank.\nCurrently, fuel is refueled all at once after a waiting period, rather than being refueled little by little.\nThe presence or absence of fuel or Loadout type ammunition within the airport is not checked.\nOne day il2dce may offer an option to check whether they are located." TabIndex="72"/>
        <CheckBox x:Name="checkBoxTrackRecording" Content="Track Recording" HorizontalAlignment="Left" Margin="1644,126,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="75" IsChecked="False"/>
        <ComboBox HorizontalAlignment="Left" Margin="1146,87,0,0" x:Name="comboBoxSelectAISkill" VerticalAlignment="Top" Width="300" FontSize="20" Foreground="#FFA0854A" Background="#00000000" TabIndex="62" MaxDropDownHeight="800" SelectionChanged="comboBoxSelectAISkill_SelectionChanged" VerticalContentAlignment="Center"/>
        <Label Content="[ AI Skill ]" HorizontalAlignment="Left" Margin="1056,46,0,0" x:Name="labelSelectAISkill" VerticalAlignment="Top" FontSize="20" Foreground="#B2FFFFFF" />
        <Controls:HelpButton x:Name="helpButtonAISkill" Content="?" HorizontalAlignment="Left" Margin="1148,52,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.219,-0.275" Message="Select the skill value and skill value range for the Air Group (AI).\nRandom is used from Tweaked types.\nAlso, each Tweaked types uses a different values on the aircraft type.\nIf you want to use user-defined skills, set them in the conf.ini file.\nIf you have set an existing name, the skill value will be overwritten." TabIndex="61"/>
        <Label Content="Air Grp:" HorizontalAlignment="Left" Margin="1056,87,0,0" x:Name="labelSelectAISkillAirGroup" VerticalAlignment="Top" FontSize="20" Foreground="#B2FFFFFF" HorizontalContentAlignment="Right" Width="84" />
        <Label Content="Artillery:" HorizontalAlignment="Left" Margin="1056,128,0,0" x:Name="labelSelectAISkillArtillery" VerticalAlignment="Top" FontSize="20" Foreground="#B2FFFFFF" HorizontalContentAlignment="Right" Width="84" />
        <Label Content="Ship:" HorizontalAlignment="Left" Margin="1056,168,0,0" x:Name="labelSelectAISkillShip" VerticalAlignment="Top" FontSize="20" Foreground="#B2FFFFFF" HorizontalContentAlignment="Right" Width="84" />
        <Label Content="Timeout" HorizontalAlignment="Left" Margin="1142,128,0,0" x:Name="labelSelectArtilleryTimeout" VerticalAlignment="Top" Foreground="#B2FFFFFF" FontSize="20" HorizontalContentAlignment="Right" />
        <Label Content="RHide" HorizontalAlignment="Left" Margin="1306,128,0,0" x:Name="labelSelectArtilleryRhide" VerticalAlignment="Top" Foreground="#B2FFFFFF" FontSize="20" HorizontalContentAlignment="Right" />
        <ComboBox HorizontalAlignment="Left" Margin="1226,128,0,0" x:Name="comboBoxSelectArtilleryTimeout" VerticalAlignment="Top" Width="80" FontSize="20" Foreground="#FFA0854A" Background="#00000000" TabIndex="63" MaxDropDownHeight="800" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
        <ComboBox HorizontalAlignment="Left" Margin="1368,128,0,0" x:Name="comboBoxSelectArtilleryRhide" VerticalAlignment="Top" Width="80" FontSize="20" Foreground="#FFA0854A" Background="#00000000" TabIndex="64" MaxDropDownHeight="800" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
        <Label Content="Sleep" HorizontalAlignment="Left" Margin="1306,168,0,0" x:Name="labelSelectShipSleep" VerticalAlignment="Top" Foreground="#B2FFFFFF" FontSize="20" HorizontalContentAlignment="Right" />
        <ComboBox HorizontalAlignment="Left" Margin="1368,168,0,0" x:Name="comboBoxSelectShipSleep" VerticalAlignment="Top" Width="80" FontSize="20" Foreground="#FFA0854A" Background="#00000000" TabIndex="67" MaxDropDownHeight="800" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
        <ComboBox HorizontalAlignment="Left" Margin="1146,168,0,0" x:Name="comboBoxSelectShipSkill" VerticalAlignment="Top" Width="160" FontSize="20" Foreground="#FFA0854A" Background="#00000000" TabIndex="66" MaxDropDownHeight="800" VerticalContentAlignment="Center"/>
        <Label Content="Slowfire" HorizontalAlignment="Left" Margin="1446,168,0,0" x:Name="labelSelectShipSlowfire" VerticalAlignment="Top" Foreground="#B2FFFFFF" FontSize="20" HorizontalContentAlignment="Right" Width="82" />
        <ComboBox HorizontalAlignment="Left" Margin="1526,168,0,0" x:Name="comboBoxSelectShipSlowfire" VerticalAlignment="Top" Width="80" FontSize="20" Foreground="#FFA0854A" Background="#00000000" TabIndex="68" MaxDropDownHeight="800" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
        <Label Content="ZOffset" HorizontalAlignment="Left" Margin="1444,128,0,0" x:Name="labelSelectArtilleryZOffset" VerticalAlignment="Top" Foreground="#B2FFFFFF" FontSize="20" HorizontalContentAlignment="Right" Width="82" />
        <ComboBox HorizontalAlignment="Left" Margin="1526,128,0,0" x:Name="comboBoxSelectArtilleryZOffset" VerticalAlignment="Top" Width="80" FontSize="20" Foreground="#FFA0854A" Background="#00000000" TabIndex="65" MaxDropDownHeight="800" HorizontalContentAlignment="Right" VerticalContentAlignment="Center"/>
        <Button x:Name="buttonUserLoad" Content="Load" HorizontalAlignment="Left" Margin="1600,10,0,0" VerticalAlignment="Top" Height="25" Foreground="#B2FFFFFF" Background="Transparent" TabIndex="100" Click="buttonUserLoad_Click" Width="40"/>
        <Button x:Name="buttonUserSave" Content="Save" HorizontalAlignment="Left" Margin="1646,10,0,0" VerticalAlignment="Top" Height="25" Foreground="#B2FFFFFF" Background="Transparent" TabIndex="101" Click="buttonUserSave_Click" Width="40"/>
        <Button x:Name="buttonUserLoadAs" Content="Load..." HorizontalAlignment="Left" Margin="1700,10,0,0" VerticalAlignment="Top" Height="25" Foreground="#B2FFFFFF" Background="Transparent" TabIndex="102" Click="buttonUserLoadAs_Click" Width="48"/>
        <Button x:Name="buttonUserSaveAs" Content="Save..." HorizontalAlignment="Left" Margin="1756,10,0,0" VerticalAlignment="Top" Height="25" Foreground="#B2FFFFFF" Background="Transparent" TabIndex="103" Click="buttonUserSaveAs_Click" Width="46"/>

        <Label Content="[ Unit Nums &amp; Type ]" HorizontalAlignment="Left" x:Name="labelSelectUnitNums" VerticalAlignment="Top" FontSize="20" Foreground="#B2FFFFFF" Margin="368,4,0,0" />
        <Controls:HelpButton x:Name="helpButtonSelectUnitNums" Content="?" HorizontalAlignment="Left" Margin="562,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.219,-0.275" Message="Specify the number of Armor/Vehicle units and the number of ships in the formation. Chek this box if you want to replace Ground Units or Artillery to Generic types." TabIndex="31"/>
        <Label Content="Armor" HorizontalAlignment="Left" Margin="372,46,0,0" x:Name="labelSelectUnitNumsArmor" VerticalAlignment="Top" FontSize="20" Foreground="#B2FFFFFF" />
        <ComboBox HorizontalAlignment="Left" Margin="438,46,0,0" x:Name="comboBoxSelectUnitNumsArmor" VerticalAlignment="Top" Width="80" FontSize="20" Foreground="#FFA0854A" Background="#00000000" VerticalContentAlignment="Center" TabIndex="32"/>
        <Label Content="Ship" HorizontalAlignment="Left" Margin="522,46,0,0" x:Name="labelSelectUnitNumsShip" VerticalAlignment="Top" FontSize="18" Foreground="#B2FFFFFF" />
        <ComboBox HorizontalAlignment="Left" Margin="570,46,0,0" x:Name="comboBoxSelectUnitNumsShip" VerticalAlignment="Top" Width="80" FontSize="20" Foreground="#FFA0854A" Background="#00000000" VerticalContentAlignment="Center" TabIndex="33"/>
        <CheckBox x:Name="checkBoxStatiomaryGeneric" Content="Stationary Units Generic" HorizontalAlignment="Left" Margin="372,130,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="36" IsChecked="False"/>
        <CheckBox x:Name="checkBoxGroundGroupGeneric" Content="Ground Group Generic" HorizontalAlignment="Left" Margin="372,90,0,0" VerticalAlignment="Top" FontSize="20" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="35" IsChecked="False"/>
        <CheckBox x:Name="checkBoxKeepUnitsAirGroups" Content="Air Group" HorizontalAlignment="Left" Margin="10,184,0,0" VerticalAlignment="Top" FontSize="18" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="22"/>
        <CheckBox x:Name="checkBoxKeepUnitsStationaryUnits" Content="Stationary" HorizontalAlignment="Left" Margin="248,184,0,0" VerticalAlignment="Top" FontSize="18" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="24"/>
        <Label Content="[ Keep nums of Groups/Units ]" HorizontalAlignment="Left" x:Name="labelSelectKeepUnitsTotal" VerticalAlignment="Top" FontSize="18" Foreground="#B2FFFFFF" Margin="10,146,0,0" />
        <CheckBox x:Name="checkBoxKeepUnitsGroundGroups" Content="Ground Grp" HorizontalAlignment="Left" Margin="120,184,0,0" VerticalAlignment="Top" FontSize="18" Background="White" Foreground="#B2FFFFFF" BorderBrush="#FF707070" VerticalContentAlignment="Center" TabIndex="23"/>
        <Controls:HelpButton x:Name="helpButtonKeepUnits" Content="?" HorizontalAlignment="Left" Margin="264,152,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.219,-0.275" Message="If there are unused units remaining in the original mission file, or if the Additional Random Units checkbox above is enabled, they will be automatically added during the mission to maintain the total number of groups and stationary units." TabIndex="21"/>
        <Label Content="[ Battle Start ]" HorizontalAlignment="Left" Margin="1056,4,0,0" x:Name="labelSelectBattleTime" VerticalAlignment="Top" FontSize="20" Foreground="#B2FFFFFF" />
        <Controls:ComboBoxEx Height="30" HorizontalAlignment="Left" Margin="1192,8,0,0" x:Name="comboBoxSelectBattleTimeBegin" VerticalAlignment="Top" Width="80" FontSize="20" Foreground="#FFA0854A" Background="#00000000" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" TabIndex="56" IsTabStop="True" MaxDropDownHeight="800" />
        <Label Content=" - " HorizontalAlignment="Left" Margin="1260,-11,0,0" x:Name="labelSelectPeriodRangeBattleTime" VerticalAlignment="Top" Width="50" FontSize="36" Foreground="#B2FFFFFF" RenderTransformOrigin="2.623,0.795" Height="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
        <Controls:ComboBoxEx Height="30" HorizontalAlignment="Left" Margin="1302,8,0,0" x:Name="comboBoxSelectBattleTimeEnd" VerticalAlignment="Top" Width="80" FontSize="20" Foreground="#FFA0854A" Background="#00000000" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" TabIndex="57" IsTabStop="True" IsSynchronizedWithCurrentItem="True" MaxDropDownHeight="800" />
    </Grid>
</GroupBox>
